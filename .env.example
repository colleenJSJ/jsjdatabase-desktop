# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=sb_anonymous_placeholder
SUPABASE_SERVICE_ROLE_KEY=sb_service_role_placeholder

# Backblaze B2 Configuration
BACKBLAZE_KEY_ID=your-backblaze-key-id
BACKBLAZE_APPLICATION_KEY=your-backblaze-application-key
BACKBLAZE_BUCKET_ID=your-backblaze-bucket-id
BACKBLAZE_BUCKET_NAME=your-backblaze-bucket-name

# Google Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Encryption Configuration
ENCRYPTION_KEY=hex-encoded-32-byte-key

# Anthropic Configuration
ANTHROPIC_API_KEY=sk-ant-placeholder

# Zoom Configuration
ZOOM_ACCOUNT_ID=your-zoom-account-id
ZOOM_CLIENT_ID=your-zoom-client-id
ZOOM_CLIENT_SECRET=your-zoom-client-secret

# Security Configuration
SECURITY_CSRF_ENFORCE=true

# Edge Runtime Configuration
EDGE_SUPABASE_SERVICE_ROLE_KEY=sb_service_role_placeholder
EDGE_SUPABASE_URL=https://your-project-ref.supabase.co
EDGE_SUPABASE_ANON_KEY=sb_anonymous_placeholder
SUPABASE_JWT_SECRET=jwt-secret-placeholder
EDGE_SERVICE_SECRET=edge-service-secret-placeholder
EDGE_ENCRYPTION_KEY=hex-encoded-32-byte-key
EDGE_BACKBLAZE_KEY_ID=your-backblaze-key-id
EDGE_BACKBLAZE_APPLICATION_KEY=your-backblaze-application-key
EDGE_BACKBLAZE_BUCKET_ID=your-backblaze-bucket-id
EDGE_BACKBLAZE_BUCKET_NAME=your-backblaze-bucket-name
EDGE_ZOOM_ACCOUNT_ID=your-zoom-account-id
EDGE_ZOOM_CLIENT_ID=your-zoom-client-id
EDGE_ZOOM_CLIENT_SECRET=your-zoom-client-secret
EDGE_ANTHROPIC_API_KEY=sk-ant-placeholder
EDGE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
EDGE_GOOGLE_CLIENT_SECRET=your-google-client-secret
EDGE_GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/callback/google
